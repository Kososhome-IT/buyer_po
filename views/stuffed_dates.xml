<odoo>
    <record id="view_stuffed_date_tree" model="ir.ui.view">
        <field name="name">stuffed_date.date.tree</field>
        <field name="model">stuffed_date.date</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date_value"/>
            </tree>
        </field>
    </record>

    <record id="view_stuffed_date_form" model="ir.ui.view">
        <field name="name">stuffed_date.date.form</field>
        <field name="model">stuffed_date.date</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="date_value"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_stuffed_date" model="ir.actions.act_window">
        <field name="name">Stuffed Date</field>
        <field name="res_model">stuffed_date.date</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>